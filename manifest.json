{
    "manifest_version": 2,
    "name": "Krystal Adblocker",
    "description": "Blocking ads.",
    "version": "0.0.1",
    "author": "Krystal Develepment",
    "browser_action": {
        "default_title": "Krystal Adblocker
    },
    "permissions": [
        "tabs",
        "webNavigation",
        "https://www.linkedin.com/"
    ],
    "background": {
        "scripts": [
            "extension.js"
        ]
    }
}
